          engineThreads="15"
          version="1">
          <components>
            <component
              refId="Package\Genera Archivo PrimLoca\Cartera"
              componentClassID="Microsoft.OLEDBSource"
              contactInfo="OLE DB Source;Microsoft Corporation; Microsoft SQL Server; (C) Microsoft Corporation; All Rights Reserved; http://www.microsoft.com/sql/support;7"
              description="OLE DB Source"
              name="Cartera"
              usesDispositions="true"
              validateExternalMetadata="False"
              version="7">
              <properties>
                <property
                  dataType="System.Int32"
                  description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                  name="CommandTimeout">0</property>
                <property
                  dataType="System.String"
                  description="Specifies the name of the database object used to open a rowset."
                  name="OpenRowset"></property>
                <property
                  dataType="System.String"
                  description="Specifies the variable that contains the name of the database object used to open a rowset."
                  name="OpenRowsetVariable"></property>
                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=15.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">;WITH cte_AgrupacionNomAuto AS(
	SELECT A.ClaveId, A.PolizaId, C.DescripcionLarga COLLATE DATABASE_DEFAULT AS AgrupacionNom
	FROM AUTO.DBO.Poliza A WITH (NOLOCK)
		INNER JOIN LSMTY_ABA.Abaseguros.Emision.Poliza B WITH (NOLOCK)
			ON A.ClaveId = B.ClaveId COLLATE DATABASE_DEFAULT AND A.PolizaId = B.PolizaId
		LEFT join LSMTY_ABA.Abaseguros.Producto.Agrupacion C WITH (NOLOCK)
			ON B.AgrupacionId = C.AgrupacionId
)
, cte_AgrupacionNomDiverso AS(
	SELECT DISTINCT PO.ClaveId, PO.PolizaId, MAX(AG.AgrupacionDsc) AS AgrupacionNom
	FROM Diverso.dbo.Poliza PO WITH(NOLOCK)
		INNER JOIN Diverso.dbo.Endoso EN WITH(NOLOCK) ON PO.PolizaFolio = EN.PolizaFolio
		INNER JOIN Diverso.dbo.IncisoEndoso IEN WITH(NOLOCK) ON EN.PolizaFolio = IEN.PolizaFolio AND EN.EndosoId = IEN.EndosoId
		INNER JOIN Diverso.dbo.CoberturaPorInciso CI WITH(NOLOCK) ON IEN.PolizaFolio = CI.PolizaFolio AND CI.IncisoId = IEN.IncisoId
		LEFT JOIN Diverso.dbo.Agrupacion AG WITH(NOLOCK) ON CI.AgrupacionId = AG.AgrupacionId
	GROUP BY PO.ClaveId, PO.PolizaId
)
, cte_ProrrogaMax AS(
	SELECT ClaveId,PolizaId, MAX(ProrrogaId) AS ProrrogaId
	FROM dbo.Prorroga WITH(NOLOCK)
	WHERE ProrrogaStatusId = 1
	GROUP BY ClaveId,PolizaId
)
, cte_Prorroga AS(
	SELECT P.ClaveId, P.PolizaId, P.DiasProrrogaNo
	FROM dbo.Prorroga P WITH(NOLOCK)
	INNER JOIN cte_ProrrogaMax C ON P.ProrrogaId = C.ProrrogaId
)

SELECT 
    Tipo = 'VENCIDO', Dias = Dias * (-1), 
    Moneda = P.MonedaNom,P.Plaza,P.Ramo,Clave = P.ClaveId,Sucursal = P.SucursalId,Poliza = P.PolizaId, 
    Endoso = P.EndosoId,Inciso = P.IncisoId,Recibo =P.ReciboId,P.TotalRecibos,'Prima Neta' = P.PNetaMto, 
    Recargo = P.RecargoMto,Derechos = P.DerechoMto,Iva = P.IvaMto,Comision = P.ComisionMto,
    'Comison Sobre Recargo' = P.ComisionSobreRecargoMto,'Prima Total' = P.PrimaTotalMto,
    'Forma de Pago' = P.FormaPago, 'Inicio Vigencia' = Convert(varchar(10),P.InicioVigencia,103),
    'Fin Vigencia' = Convert(varchar(10),P.FinVigencia,103), 
    'Fecha Emision' = Convert(varchar(10),P.EmisionFecha,103), 
    Agente = P.AgenteId, 'Nombre Agente' = convert(varchar(255),P.AgenteNom),
    Conducto = P.ConductoId, 
    'Nombre Conducto' = convert(varchar(255),P.ConductoNom),Asegurado = P.AseguradoId,
    'Nombre Asegurado' =convert(varchar(255),P.AseguradoNom),
    Fecha = CONVERT(VARCHAR (15),GETDATE()-1, 103),
    LEFT(P.Referencia,15) AS Referencia
	,P.DescPaquete,
DescripcionMTC=P.DescripcionMTC,--dbo.Fn_OmitirCaracter(P.DescripcionMTC),
P.Modelo,P.Serie,P.Motor,P.Propietario,
    'TipoSiniestro' = CASE WHEN P.TipoSiniestroId &gt; 1 THEN 'SINIESTRO' ELSE 
    CASE WHEN P.TipoSiniestroId = 1 THEN 'SINIESTRO PT' ELSE '' END END,
    LimiteFecha = Convert(varchar(10),P.LimiteFecha,103),P.TipoEndoso,P.Placas,P.MTC, 
    ISNULL(P.PrimaTotalIncisoMto,0) AS 'PrimaTotalInciso' 
    --MonedaId,RamoId,FormaPagoId, TipoSiniestroId,Plaza  
    ,isnull(P.ProductoNom,'') as Producto
    ,TipoPolizaInternacional = (SELECT CASE ISNULL(D.PROGRAMTYPE,2) 
                        WHEN 0 THEN 'MULTINACIONAL - PAGO DIRECTO'
                        WHEN 1 THEN 'MULTINACIONAL - BORDERAUX'
                        WHEN 2 THEN ''
                    END
                FROM Seguro.dbo.PolizaMultinacionalDetalle D WITH(NOLOCK)            
                WHERE D.EndosoID = P.EndosoID)
    ,LineaNegocio=(
		Select convert(Varchar(25),ln.Descripcion) 
		FROM AUTO.DBO.POLIZA Po with (nolock)
			INNER JOIN SEGURO.DBO.LineaNegocioSIS ln with (nolock) ON LN.LobSISid=Po.LobSISId
		WHERE  Po.ClaveId=P.ClaveId AND Po.polizaid = P.PolizaId        
		UNION
		Select convert(Varchar(25),ln.Descripcion) 
		FROM DIVERSO.DBO.POLIZA Po with (nolock)
			INNER JOIN SEGURO.DBO.LineaNegocioSIS ln with (nolock) ON LN.LobSISid=Po.LobSISId
		WHERE  Po.ClaveId=P.ClaveId AND Po.polizaid = P.PolizaId AND not exists(Select  PO.PolizaId,LN.LobSISid,PO.ClaveId,LN.Descripcion
		FROM AUTO.DBO.POLIZA Po with (nolock)
			INNER JOIN SEGURO.DBO.LineaNegocioSIS ln with (nolock) ON LN.LobSISid=Po.LobSISId
		WHERE  Po.ClaveId=P.ClaveId AND Po.polizaid = P.PolizaId))

	,CASE P.RamoId WHEN 3 THEN AGA.AgrupacionNom ELSE AGD.AgrupacionNom END AS AgrupacionNom
	,CONVERT(VARCHAR(25),E.DiasGraciaIni) AS DiasGracia
	,CONVERT(VARCHAR(25),PR.DiasProrrogaNo) AS DiasProrroga
FROM tmpReportePrimas P WITH (NOLOCK)
	INNER JOIN dbo.Recibo RE WITH (NOLOCK)
		ON RE.PolizaId=P.PolizaId AND RE.ClaveId = P.ClaveId AND RE.endosoid = P.endosoid AND RE.IncisoId = P.IncisoId AND RE.ReciboId = P.ReciboId
	INNER JOIN dbo.Endoso E WITH(NOLOCK) ON RE.ClaveId = E.ClaveId AND RE.PolizaId = E.PolizaId AND RE.EndosoId = E.EndosoId
	LEFT JOIN cte_AgrupacionNomAuto AGA ON P.ClaveId = AGA.ClaveId AND P.PolizaId = AGA.PolizaId AND P.RamoId = 3
	LEFT JOIN cte_AgrupacionNomDiverso AGD ON P.ClaveId = AGD.ClaveId AND P.PolizaId = AGD.PolizaId AND P.RamoId &lt;&gt; 3
	LEFT JOIN cte_Prorroga PR ON PR.PolizaId = E.PolizaId AND PR.ClaveId = E.ClaveId
WHERE 	P.Dias &lt; 0

UNION ALL

